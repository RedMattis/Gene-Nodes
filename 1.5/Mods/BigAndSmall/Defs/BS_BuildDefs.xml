      <texPath>GeneNodes/GET_GeneNode_BudgetDeathless</texPath>
    </graphicData>
    <descriptionHyperlinks>
      <GeneDef>Deathless</GeneDef>
      <GeneDef>BS_LesserDeathless</GeneDef>
    </descriptionHyperlinks>
    <comps>
      <li Class="GeneExtractorTiers.CompProperties_GeneNode">
